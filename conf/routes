# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~

# Home page
GET      /home                      controllers.SearchApplication.index()
POST    /postdocument               controllers.SearchApplication.postDocument()
GET     /performsearch              controllers.SearchApplication.performSearch(querystring: String, fromPage: Int, toPage: Int)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
